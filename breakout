package java202126;

import java.util.Scanner;

public class java202126 {

	public static void main(String[]args)
	{
		Scanner in = new Scanner(System.in);
		int money=in.nextInt();
		OUT:
		for(int i=1;i<10;i++)
			for(int j=1;j<10;j++)
				for(int k=1;k<10;k++)
					if(i+j*2+k*5==money)
					{
						System.out.println(i+" "+j+" "+k);
						break OUT;
					}
	}
	
	
}
