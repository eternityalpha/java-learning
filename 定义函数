package java20210306;

import java.util.Scanner;

public class java20210306 {

	public static int sum(int a,int b)
	{
		return a+b;
	}//定义函数，public static；
	
	public static void main(String[]args)
	{	
		Scanner in = new Scanner(System.in);
		int a,b;
		int[] sum = new int[3];
		
		for(int i=0;i<3;i++)
		{
			a=in.nextInt();
			b=in.nextInt();
			sum[i]=sum(a,b);	
		}
		for(int i=0;i<3;i++)
			System.out.println(sum[i]);
	}
	
}
