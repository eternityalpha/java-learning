package javapractise;

import java.util.Scanner;

//array of java
public class J0213 {

	public static void main(String[]args)
	{
		Scanner in = new Scanner(System.in);
		//for each循环，遍历数组。
		int[] a = {12,33,53,11,33,11,64,34};
		for(int k:a)
		{
			System.out.print(k+" ");
		}//每一次循环，k被赋值为a[0]、a[1]、a[2]、a[3]……
		
	}
	
}
