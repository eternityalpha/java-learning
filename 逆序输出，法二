import java.util.Scanner;

public class reverse 
{
	public static void main(String[]args)
	{
		Scanner in = new Scanner(System.in);
		int a;
		a = in.nextInt();
		int mid = a;
		int outcome=0;
		while(mid!=0)
		{
			int digit=mid%10;
			outcome=outcome*10+digit;
			mid/=10;
		}
		System.out.println(outcome);
	}
}
